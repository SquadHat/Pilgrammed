--[[ Kavo UI ]]--
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pilgrammed", "Synapse")
local Main = Window:NewTab("Main")
local TP = Window:NewTab("Teleport")
local Settings = Window:NewTab("Settings")

--[[ Section ]]--
local Bank = Main:NewSection("Bank")
local Loadout = Main:NewSection("Miscellaneous")
local Teleport = TP:NewSection("Island")
local Settings = Settings:NewSection("Settings")

--[[ Main ]]--

Bank:NewToggle("Auto Deposit", "Auto put your money in your bank", function(state)
    if state then
_G.Deposit = true
while _G.Deposit == true do
game:GetService("ReplicatedStorage").Remotes.Bank:InvokeServer(true, 1)
wait()
end
    else
        _G.Deposit = false
    end
end)
Bank:NewButton("Withdraw Half", "Withdraw half of your money in bank", function()
game:GetService("ReplicatedStorage").Remotes.Bank:InvokeServer(false, 0.5)
end)



Loadout:NewKeybind("Cabinet", "Open Cabinet Anywhere!", Enum.KeyCode.J, function()
game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Equipment.Visible = true
end)

Loadout:NewButton("Teleport To Mela", "teleport!1?!!?1", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").NPCs.Mela:GetPivot()
end)

Loadout:NewButton("Trigger Cultists", "open N door to trigger cultists", function()
fireclickdetector(game:GetService("Workspace").Map.Hideout.Door.Door.ClickDetector)
end)


--[[ Teleport ]]--

Teleport:NewDropdown("Island", "Teleport to specific islands", {"Big Iron", "Blackened Island", "Bear Island", "Dark Tower Island", "LOFI Island", "Vampire Island", "Mountain Basin", "Field", "Labyrinth", "Polar Bear Island", "Backdoor Island", "Patris Island", "Enchanted Island", "Eggroost Island"}, function(SelectedLocation)
    if SelectedLocation == "Big Iron" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2399, -17, -3098)
   elseif SelectedLocation == "Blackened Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-52, -11, -4458)
   elseif SelectedLocation == "Bear Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1463, 69, -130)
   elseif SelectedLocation == "Dark Tower Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(889, -5, -5835)
   elseif SelectedLocation == "LOFI Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3398, -293, -1502)
   elseif SelectedLocation == "Vampire Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2795, -22, -2106)
   elseif SelectedLocation == "Mountain Basin" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1404, -22, 988)
   elseif SelectedLocation == "Field" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(7487, -460, 1006)
   elseif SelectedLocation == "Labyrinth" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-147, -17, 2671)
   elseif SelectedLocation == "Polar Bear Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(550, -21, 1617)
   elseif SelectedLocation == "Backdoor Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1973, -21, -5007)
   elseif SelectedLocation == "Patris Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3251, -22, -4615)
   elseif SelectedLocation == "Enchanted Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-824, -2, 1735)
   elseif SelectedLocation == "Eggroost Island" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2070, 57, -3242)
   end
end)

--[[ Places ]]--

