--[[ Gem ]]--

Remo:NewButton("Remove Gem", "Remove The Currently Holding Weapon With Gem Reforged", function()
game:GetService("ReplicatedStorage").Remotes.Gem:FireServer()
end)

Gemo:NewButton("Charm Gem", "Install Charm Gem on Holding Weapon", function()
game:GetService("ReplicatedStorage").Remotes.Gem:FireServer("Charm Gem")
end)

Gemo:NewButton("Blood Gem", "Install Blood Gem on Holding Weapon", function()
game:GetService("ReplicatedStorage").Remotes.Gem:FireServer("Blood Gem")
end)

