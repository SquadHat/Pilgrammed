        [1] = {
            [1] = "Shrimp",
            [2] = "Sugar",
            [3] = "Sugar",
            [4] = "Sugar",
            [5] = "Sugar"
        }
    }
    
    game:GetService("ReplicatedStorage").Remotes.Cauldron:FireServer(unpack(args))
