if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Title") ~= nil then
game.StarterGui:SetCore("SendNotification", {
Title = "INSTRUCTION";
Text = "Enter a game and execute script again";
Icon = "rbxassetid://2541869220";
Duration = 5;
})
else
--[[ Kavo UI ]]--
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pilgrammed", "Synapse")
local Main = Window:NewTab("Main")
local Misc = Window:NewTab("Misc")
local Shop = Window:NewTab("Shop")
local TP = Window:NewTab("Teleport")
local Potion = Window:NewTab("Potion")
local Gemmer = Window:NewTab("Gem")
local Meteor = Window:NewTab("Meteor")
local Editor = Window:NewTab('Character Editor')
local Settings = Window:NewTab("Settings")

--[[ Section ]]--
local Bank = Main:NewSection("Bank")
local Meteorz = Meteor:NewSection("--[[ Meteor ]]--")
local Loadout = Misc:NewSection("Miscellaneous")
local Daily = Shop:NewSection("Mela Shop")
local Scaven = Shop:NewSection("Ingredients")
local Shorp = Shop:NewSection("Items")
local Armos = Shop:NewSection("Armor/Accessory")
local Rare = Shop:NewSection("Others")
local Shosp = Shop:NewSection("Special Items")
local wtf = Shop:NewSection("What The Fuck")
local Teleport = TP:NewSection("Island")
local Boss = TP:NewSection("Boss")
local Poton = Potion:NewSection("Potion Recipes")
local Remo = Gemmer:NewSection("Remove Gem")
local Gemo = Gemmer:NewSection("Install Gem")
local Editz = Editor:NewSection("Character Changer (DONT TOUCH THIS)")
local Settings = Settings:NewSection("Settings")

--[[ Remotes ]]--
local Races = game:GetService("ReplicatedStorage").Remotes.Skill
local TPTO = game.Players.LocalPlayer
local Gesm = game:GetService("ReplicatedStorage").Remotes.Gem
local npcsr = game:GetService("Workspace").NPCs
local daily1 = game:GetService("ReplicatedStorage").Remotes.Buy
local meteor = game:GetService("ReplicatedStorage").GameInfo.Meteor
local meteorvalue = Meteorz:NewLabel(meteor.Value)

--[[ Main ]]--

spawn(function()
   while wait(.1) do
       meteorvalue:UpdateLabel("Meteor Spawns In "..tostring((game:GetService("ReplicatedStorage").GameInfo.Meteor.Value * 2)).." Seconds...")
   end
end)

Bank:NewToggle("Auto Deposit", "Auto put your money in your bank", function(state)
    if state then
_G.Deposit = true
while _G.Deposit == true do
game:GetService("ReplicatedStorage").Remotes.Bank:InvokeServer(true, 9000000000)
wait()
end
    else
        _G.Deposit = false
    end
end)
Bank:NewButton("Withdraw Half", "Withdraw half of your money in bank", function()
game:GetService("ReplicatedStorage").Remotes.Bank:InvokeServer(false, 0.5)
end)

Bank:NewKeybind("Bank", "Open Bank Anywhere", Enum.KeyCode.K, function()
local frame = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Bank
frame.Visible = not frame.Visible
end)

--[[ Misc ]]--

Loadout:NewKeybind("Cabinet", "Open Cabinet Anywhere!", Enum.KeyCode.J, function()
local cabinetprompt = game:GetService("Workspace").Map.PrairieVillage.Cabinet:GetChildren()[2].ProximityPrompt
fireproximityprompt(cabinetprompt)
end)

