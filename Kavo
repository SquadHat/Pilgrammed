local skillkey = game:GetService("ReplicatedStorage").Remotes.SkillKey

Editz:NewLabel("Skill Key")
Editz:NewKeybind("Effect", "", Enum.KeyCode.KeypadPlus, function()
for _,v in pairs(game:GetService("Players").LocalPlayer.EquippedAccessories:GetChildren()) do
    if v:IsA"StringValue" and v.Value == "Power Locket" then
armor:FireServer(3,equipaccessory:FindFirstChild("Kolis Charm"),false)
skillkey:FireServer(SkillX, 1)
wait(10.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Kai Blessing"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Power Locket"),false)
    else if v.Value == "Combo Glove" then
armor:FireServer(3,equipaccessory:FindFirstChild("Kai Blessing"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Knight Talisman"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Combo Glove"),false)
    else if v.Value == "Outlaw Glasses" then
armor:FireServer(3,equipaccessory:FindFirstChild("Kai Blessing"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Knight Talisman"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Outlaw Glasses"),false)
end end end end
end)

Editz:NewKeybind("Dodge", "", Enum.KeyCode.G, function()
for _,v in pairs(game:GetService("Players").LocalPlayer.EquippedAccessories:GetChildren()) do
    if v:IsA"StringValue" and v.Value == "Power Locket" then
armor:FireServer(3,equipaccessory:FindFirstChild("Ivory Anklet"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Power Locket"),false)
    else if v.Value == "Combo Glove" then
armor:FireServer(3,equipaccessory:FindFirstChild("Ivory Anklet"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Combo Glove"),false)
    else if v.Value == "Outlaw Glasses" then
armor:FireServer(3,equipaccessory:FindFirstChild("Pirate Bracelet"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Outlaw Glasses"),false)
end end end end
end)

Editz:NewKeybind("Heal", "", Enum.KeyCode.Z, function()
for _,v in pairs(game:GetService("Players").LocalPlayer.EquippedAccessories:GetChildren()) do
    if v:IsA"StringValue" and v.Value == "Power Locket" then
armor:FireServer(3,equipaccessory:FindFirstChild("Blood Chalice"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Power Locket"),false)
    else if v.Value == "Combo Glove" then
armor:FireServer(3,equipaccessory:FindFirstChild("Blood Chalice"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Combo Glove"),false)
    else if v.Value == "Outlaw Glasses" then
armor:FireServer(3,equipaccessory:FindFirstChild("Blood Chalice"),false)
skillkey:FireServer(SkillX, 1)
wait(0.5)
armor:FireServer(3,equipaccessory:FindFirstChild("Outlaw Glasses"),false)
end end end end
end)
--[[ Settings ]]--

Settings:NewKeybind("Bind UI", "", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)
end
